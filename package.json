    "@vitejs/plugin-react-swc": "^3.1.0",
    "less": "^4.1.3",
    "prefix-classnames": "^0.0.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^4.9.5",
    "vite": "3.2.5"
  },
  "peerDependencies": {
    "react": ">=17",
    "react-dom": ">=17"
  },
  "dependencies": {
    "ahooks": "^3.7.4",
    "classnames": "^2.3.2",
    "dplayer": "^1.27.1",
